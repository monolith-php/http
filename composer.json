{
  "name": "monolith/http",
  "description": "HTTP abstractions for the Monolith framework.",
  "keywords": [
    "monolith",
    "http"
  ],
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com"
    },
    {
      "name": "Mitchell van Wijngaarden",
      "email": "mitchell@kooding.nl"
    }
  ],
  "require": {
    "php": "^7.1",
    "monolith/collections": "^2"
  },
  "require-dev": {
    "phpspec/phpspec": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\Http\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\Http\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
